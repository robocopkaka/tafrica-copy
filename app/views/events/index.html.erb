<div id="events-list">
  <div class="container">
    <h2 id="events-list-header">All Events</h2>
    <% if @events.empty? %>
      <div id="empty-events">No events found</div>
    <% else %>
      <div class="row">
        <%= render @events %>
        <div id="pagination-links">
          <%= will_paginate @events, renderer: WillPaginate::ActionView::BootstrapLinkRenderer  %>
        </div>
      </div>
    <% end %>
  </div>
</div>
